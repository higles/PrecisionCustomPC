@model PrecisionCustomPC.Models.PartsViewModels.Tower
@{
    ViewData["Title"] = "Admin - Add Tower";
    Layout = "";
}
<head>
    <title>PC - @ViewData["Title"]</title>
</head>

<h1>Add Tower</h1>

<form asp-action="TowerAdd" asp-controller="Admin">
    @Html.EditorForModel()
    <input type="submit" name="Submit" value="Add Tower"/>
</form>
<style>
    .editor-field {
        margin-bottom: 0.5em;
    }
</style>
<hr/>
<a asp-action="Towers" asp-controller="Admin">Cancel</a>