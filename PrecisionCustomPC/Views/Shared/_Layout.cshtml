@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PrecisionCustomPC</title>
    <base href="/" />

    <!-- Stylesheets -->
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/menu.css" />
    <link rel="stylesheet" href="/css/rotating-bg.css" />

    <!-- Fonts -->
    <link rel="stylesheet" href="fonts/Disco/web fonts/disco_regular_macroman/stylesheet.css" />
    <link rel="stylesheet" href="fonts/Bungee/web fonts/stylesheet.css" />
    <link rel="stylesheet" href="fonts/Exo_Condensed/stylesheet.css" />

    <!-- JavaScript -->
    <!--script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script-->
    <script src="http://www.w3schools.com/lib/w3data.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
    <menu>
        <div id="menu-center">
            <ul><a asp-controller="About" asp-action="Index">About</a></ul>
            <ul><a asp-controller="Store" asp-action="Index">Store</a></ul>
            <ul><a asp-controller="Build" asp-action="Index">Build</a></ul>
            <ul><a asp-controller="Contact" asp-action="Index">Contact</a></ul>
        </div>
    </menu>

    <div id="content">
        <header>
            <img id="logo" src="images/logo-web2.png" />
        </header>

        @RenderBody()

        <ul class="bg-slideshow">
            <li>
                <span>Image 01</span>
                <div>
                    <h3>Power</h3>
                </div>
            </li>
            <li>
                <span>Image 02</span>
                <div>
                    <h3>Speed</h3>
                </div>
            </li>
            <li>
                <span>Image 03</span>
                <div>
                    <h3>Custom</h3>
                </div>
            </li>
            <li>
                <span>Image 04</span>
                <div>
                    <h3>Graphics</h3>
                </div>
            </li>
            <li>
                <span>Image 05</span>
                <div>
                    <h3>Perfection</h3>
                </div>
            </li>
            <li>
                <span>Image 06</span>
                <div>
                    <h3>Performance</h3>
                </div>
            </li>
        </ul>
    </!--div>

    @RenderSection("Scripts", required: false)
</body>
</html>
